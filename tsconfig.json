{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "strict": true,
        "declaration":true,
        "noImplicitAny": true,
        "sourceMap": true,
        "outDir": "./dist",
        "allowSyntheticDefaultImports": true,
        "allowJs": true,
        "esModuleInterop": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}




